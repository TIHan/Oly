#target "dotnet: net7"

#reference "../core/prelude.olyx"

#[import("C", "user32.dll", "MessageBox")]
MessageBox(hWnd: nint, lpText: string, lpCaption: string, uType: uint32): int32

#[not inline]
chicken(): () =
    print("chicken")

main(): () =
    let result = System.Diagnostics.Debugger.Launch()
