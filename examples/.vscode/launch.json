{
    "version": "0.2.0",
    "compounds": [
        {
        "name": "Oly Interpreter Launch",
        "configurations": [""],
        "preLaunchTask": "oly",
        "stopAll": true
        }
    ],
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "oly-compile",
            "program": "${fileDirname}/bin/dotnet/${fileBasename}/${fileBasenameNoExtension}.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}